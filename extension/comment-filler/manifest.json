{
  "manifest_version": 3,
  "name": "Auto Comment Filler",
  "version": "1.0",
  "description": "Mengisi komentar otomatis ke text area tertentu.",
  "permissions": ["scripting", "activeTab"],
  "action": {
    "default_popup": "popup.html"
  },
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["content.js"]
    }
  ]
}
